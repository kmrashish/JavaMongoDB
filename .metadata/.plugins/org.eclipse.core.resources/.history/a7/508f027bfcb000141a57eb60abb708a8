
public class Ashish {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int max=5;
		for(int i=1, gap=max-2;i<=max-2;i++, gap-=2)
			{
			    for(int x=1;x<=i;x++)
				System.out.println("*");
			    while(gap>0)System.out.print(" ");
			    for(int x=1;x<=i;x++)
				System.out.println("*");
			}

	}

}
